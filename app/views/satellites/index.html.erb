<p id="notice"><%= notice %></p>

<h1>Satellites</h1>

<table>
  <thead>
    <tr>
      <th>Norad catalog</th>
      <th>International designator</th>
      <th>Name</th>
      <th>Object name</th>
      <th>Object type</th>
      <th>Object</th>
      <th>Object number</th>
      <th>Country</th>
      <th>Launch date</th>
      <th>Launch site</th>
      <th>Decay date</th>
      <th>Launch year</th>
      <th>Launch number</th>
      <th>Launch piece</th>
      <th>Period</th>
      <th>Inclination</th>
      <th>Apogee</th>
      <th>Perigee</th>
      <th>Radar cross section value</th>
      <th>Radar cross section size</th>
      <th>Comment</th>
      <th>Comment code</th>
      <th>File</th>
      <th>Current</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @satellites.each do |satellite| %>
      <tr>
        <td><%= satellite.norad_catalog_id %></td>
        <td><%= satellite.international_designator %></td>
        <td><%= satellite.name %></td>
        <td><%= satellite.object_name %></td>
        <td><%= satellite.object_type %></td>
        <td><%= satellite.object_id %></td>
        <td><%= satellite.object_number %></td>
        <td><%= satellite.country %></td>
        <td><%= satellite.launch_date %></td>
        <td><%= satellite.launch_site %></td>
        <td><%= satellite.decay_date %></td>
        <td><%= satellite.launch_year %></td>
        <td><%= satellite.launch_number %></td>
        <td><%= satellite.launch_piece %></td>
        <td><%= satellite.period %></td>
        <td><%= satellite.inclination %></td>
        <td><%= satellite.apogee %></td>
        <td><%= satellite.perigee %></td>
        <td><%= satellite.radar_cross_section_value %></td>
        <td><%= satellite.radar_cross_section_size %></td>
        <td><%= satellite.comment %></td>
        <td><%= satellite.comment_code %></td>
        <td><%= satellite.file %></td>
        <td><%= satellite.current %></td>
        <td><%= link_to 'Show', satellite %></td>
        <td><%= link_to 'Edit', edit_satellite_path(satellite) %></td>
        <td><%= link_to 'Destroy', satellite, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Satellite', new_satellite_path %>
